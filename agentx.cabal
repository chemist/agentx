name:                agentx
version:             0.1.0.0
synopsis:            AgentX protocol for snmp subagents
description:         Native implementation agentx protocol (rfc2741)       
license:             BSD3
license-file:        LICENSE
author:              chemist
maintainer:          chemistmail@gmail.com
-- copyright:           
category:            Network
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

library
  exposed-modules:     Network.Protocol.Snmp.AgentX
  -- other-modules:       
  -- other-extensions:    
  build-depends:       base >=4.6 && <4.8
                     , snmp
                     , binary
                     , bytestring
                     , bitwise
  hs-source-dirs:      src
  default-language:    Haskell2010

source-repository head
  type:              git
  location:          git@github.com:chemist/agentx.git
